{
  "name": "blog2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "rss": "TS_NODE_PROJECT=\"tsconfig-tsnode.json\" TS_NODE_TRANSPILE_ONLY=true ts-node src/plugins/rssMetadata/index.ts",
    "type": "tsc --noEmit",
    "test": "env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" mocha",
    "size": "size-limit"
  },
  "dependencies": {
    "firebase-admin": "10.0.1",
    "formik": "2.2.9",
    "gray-matter": "4.0.3",
    "next": "12.0.7",
    "next-mdx-remote": "3.0.8",
    "prism-react-renderer": "1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-unwrap-images": "3.0.1",
    "swr": "1.1.2",
    "unist-util-visit": "4.1.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@size-limit/preset-app": "7.0.5",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jsdom": "16.2.14",
    "@types/mocha": "9.0.0",
    "@types/react": "17.0.38",
    "earljs": "0.1.12",
    "eslint": "8.6.0",
    "eslint-config-next": "12.0.7",
    "jsdom": "19.0.0",
    "mocha": "9.1.3",
    "remark": "14.0.2",
    "size-limit": "7.0.5",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "size-limit": [
    {
      "path": ".next/static/chunks/framework-*.js",
      "limit": "45 kB"
    },
    {
      "path": ".next/static/chunks/761-*.js",
      "limit": "37 kB"
    },
    {
      "path": ".next/static/chunks/152-*.js",
      "limit": "30 kB"
    },
    {
      "path": ".next/static/chunks/main-*.js",
      "limit": "29 kB"
    },
    {
      "path": ".next/static/chunks/pages/[slug]-*.js",
      "limit": "5 kB"
    },
    {
      "path": ".next/static/chunks/pages/search-*.js",
      "limit": "4 kB"
    },
    {
      "path": ".next/static/chunks/pages/index-*.js",
      "limit": "3 kB"
    }
  ]
}
