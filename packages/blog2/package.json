{
  "name": "blog2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "rss": "TS_NODE_PROJECT=\"tsconfig-tsnode.json\" TS_NODE_TRANSPILE_ONLY=true ts-node src/plugins/rssMetadata/index.ts",
    "type": "tsc --noEmit",
    "test": "env TS_NODE_PROJECT=\"tsconfig-tsnode.json\" mocha",
    "size": "size-limit"
  },
  "dependencies": {
    "firebase-admin": "10.0.2",
    "formik": "2.2.9",
    "gray-matter": "4.0.3",
    "next": "12.0.8",
    "next-mdx-remote": "3.0.8",
    "prism-react-renderer": "1.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "remark-unwrap-images": "3.0.1",
    "swr": "1.2.0",
    "unist-util-visit": "4.1.0",
    "yup": "0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@firebase/app-compat": "0.1.16",
    "@firebase/app-types": "0.7.0",
    "@size-limit/preset-app": "7.0.5",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jsdom": "16.2.14",
    "@types/mocha": "9.1.0",
    "@types/node": "*",
    "@types/react": "17.0.38",
    "csstype": "3.0.10",
    "earljs": "0.2.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.8",
    "jsdom": "19.0.0",
    "mocha": "9.2.0",
    "remark": "14.0.2",
    "size-limit": "7.0.5",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "unified": "10.1.1"
  },
  "size-limit": [
    {
      "path": [
        ".next/static/chunks/*.js",
        "!.next/static/chunks/framework-*.js",
        "!.next/static/chunks/main-*.js",
        "!.next/static/chunks/polyfills-*.js",
        "!.next/static/chunks/webpack-*.js"
      ],
      "limit": "88 kB"
    },
    {
      "path": ".next/static/chunks/framework-*.js",
      "limit": "45 kB"
    },
    {
      "path": ".next/static/chunks/polyfills-*.js",
      "limit": "32 kB"
    },
    {
      "path": ".next/static/chunks/main-*.js",
      "limit": "31 kB"
    },
    {
      "path": ".next/static/chunks/pages/[slug]-*.js",
      "limit": "5 kB"
    },
    {
      "path": ".next/static/chunks/pages/search-*.js",
      "limit": "4 kB"
    },
    {
      "path": ".next/static/chunks/pages/index-*.js",
      "limit": "3 kB"
    },
    {
      "path": ".next/static/chunks/webpack-*.js",
      "limit": "2 kB"
    }
  ]
}
